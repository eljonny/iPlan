<Window x:Class="GUIProj1.Window1" Background="SlateGray"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="iPlan" Height="545" Width="955" Name="iPlanMain" MaxHeight="768"
        WindowStartupLocation="CenterOwner" MinWidth="955" MinHeight="545"
        MaxWidth="955" ToolTipService.HasDropShadow="True"
        ToolTipService.InitialShowDelay="1000" ToolTipService.BetweenShowDelay="500">
    <!-- Main window grid that holds buttons/text/calander grid/etc -->
        <Grid Height="auto" Width="950" MinHeight="512" MaxHeight="768"
              VerticalAlignment="Top" HorizontalAlignment="Left">
        <Grid.Background>
            <SolidColorBrush Color="WhiteSmoke"></SolidColorBrush>
        </Grid.Background>
        <!-- Dynamically sized grid cells -->
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="110*" />
            <ColumnDefinition Width="846*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="37*" MinHeight="35" MaxHeight="39" />
            <RowDefinition Height="448*" MaxHeight="729" />
        </Grid.RowDefinitions>
        <!-- program menus -->
            <Menu Name="fileMenu"
                  AllowDrop="True" FontFamily="Verdana"
                  FontSize="14" FontWeight="bold" Width="50" HorizontalAlignment="Left"
                  Margin="3,5,0,3" >
                <MenuItem Header="_File"
                          ToolTip="Make new, save, and open calendars and teams.">
                    <MenuItem Header="_New Calendar" />
                    <MenuItem Header="New _Team Member" Click="newTeamMember_Click" />
                    <MenuItem Header="_Open Calendar" />
                    <MenuItem Header="O_pen Team Member" Click="openTeam_Click" />
                    <MenuItem Header="_Save Calendar" />
                    <MenuItem Header="S_ave Calendar As..." />
                    <MenuItem Header="Sa_ve Team Member..." />
                    <Separator />
                    <MenuItem Header="_Quit" Click="file_quit" />
                </MenuItem>
            </Menu>
            <Menu AllowDrop="True" Name="optionMenu"
                  Width="60" FontFamily="Verdana" FontSize="14" FontWeight="bold"
                  HorizontalAlignment="Right" Margin="0,5,1,3">
                <MenuItem Header="_Tools"
                  ToolTip="Change calendar properties, change view mode,
                          enter/leave edit mode. Edit teams and team members." >
                    <MenuItem Header="_View Mode" Click="chgViewMode" />
                    <MenuItem Header="_Edit Mode" Click="enterOrLeaveEditMode"/>
                    <MenuItem Header="Edit _Team" />
                    <MenuItem Header="Eve_nt Details" />
                    <Separator/>
                    <MenuItem Header="C_ustomize" Click="MenuItem_Click" />
                </MenuItem>            
            </Menu>
            <Menu AllowDrop="True" Name="helpMenu" Grid.Column="1"
                      Width="50" FontFamily="Verdana" FontSize="14" FontWeight="bold"
                      HorizontalAlignment="Left" Margin="0,5,0,3">
                <MenuItem Header="_Help"
                    ToolTip="Access help file contents, as well as version and
                          author information.">
                    <MenuItem Header="_About" Click="about_Click"/>
                    <Separator />
                    <MenuItem Header="Help _Contents" />
                </MenuItem>
            </Menu>
        <Label VerticalAlignment="Center" Grid.Column="1" Width="65" FontFamily="Verdana"
               HorizontalAlignment="Left" Margin="60,0,0,0" FontSize="15"
               ToolTip="Change the current calendar schedule month of the year.">
                    Month: </Label>
        <ComboBox Grid.Column="1" Width="110" Height="27" HorizontalAlignment="Left"
                  Margin="125,0,0,0" Text="Month" Name="moComboBox" FontFamily="Verdana"
                  FontSize="15">
            <ComboBoxItem Name="jan">
                January
            </ComboBoxItem>
            <ComboBoxItem Name="feb">
                February
            </ComboBoxItem>
            <ComboBoxItem Name="mar">
                March
            </ComboBoxItem>
            <ComboBoxItem Name="apr">
                April
            </ComboBoxItem>
            <ComboBoxItem Name="may">
                May
            </ComboBoxItem>
            <ComboBoxItem Name="jun" Selector.IsSelected="True">
                June
            </ComboBoxItem>
            <ComboBoxItem Name="jul">
                July
            </ComboBoxItem>
            <ComboBoxItem Name="aug">
                August
            </ComboBoxItem>
            <ComboBoxItem Name="sep">
                September
            </ComboBoxItem>
            <ComboBoxItem Name="oct">
                October
            </ComboBoxItem>
            <ComboBoxItem Name="nov">
                November
            </ComboBoxItem>
            <ComboBoxItem Name="dec">
