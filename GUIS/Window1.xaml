                    FontFamily="Verdana" FontSize="13" Margin="3,170,5,0"
                Grid.Row="1" ToolTip="Save the currently loaded calendar."
                Foreground="Black">
                Save Calendar
            </Button>
            <Button Height="50" Name="propCalButton" VerticalAlignment="Top"
                    FontFamily="Verdana" FontSize="13" Margin="3,235,5,0"
                    Grid.Row="1" ToolTip="Change the properties of the currently
                                            loaded calendar.">
                Properties
            </Button>
            
            <Button Height="50" Name="schedCalButton" VerticalAlignment="Top"
                            FontFamily="Verdana" FontSize="13" Margin="3,300,5,0"
                            Grid.Row="1" ToolTip="View schedule details for current month or week.">
                Sched. Details
            </Button>

            <Button Height="50" Name="membersCalButton" VerticalAlignment="Top"
                    FontFamily="Verdana" FontSize="13" Margin="3,5,5,0"
                    Grid.Row="1" ToolTip="View the currently participating team members." Click="membersCalButton_Click">
                Team
            </Button>
            <Button Height="50" Name="notifyCalButton" VerticalAlignment="Top"
                        FontFamily="Verdana" FontSize="13" Margin="3,355,5,0"
                        Grid.Row="1" ToolTip="Send notifications to team members."
